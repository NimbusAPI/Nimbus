<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>Autofac extensions for Nimbus.</Description>
    <AssemblyTitle>Nimbus.Autofac</AssemblyTitle>
    <AssemblyName>Nimbus.Autofac</AssemblyName>
    <PackageId>Nimbus.Autofac</PackageId>
    <Authors>Damian Maclennan, Andrew Harcourt</Authors>
    <PackageProjectUrl>http://nimbusapi.com/</PackageProjectUrl>
    <PackageTags>azure,servicebus,autofac</PackageTags>
    <TargetFrameworks>netstandard2.0; net452</TargetFrameworks>
    <VersionPrefix>3.0.1</VersionPrefix>
    <VersionSuffix>issue-275.1</VersionSuffix>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Autofac" Version="4.6.2" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="../../Nimbus/Nimbus.csproj" />
    <ProjectReference Include="../../Nimbus.InfrastructureContracts/Nimbus.InfrastructureContracts.csproj" />
    <ProjectReference Include="../../Nimbus.MessageContracts/Nimbus.MessageContracts.csproj" />
  </ItemGroup>
</Project>