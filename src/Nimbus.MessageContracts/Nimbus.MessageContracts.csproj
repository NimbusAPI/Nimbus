<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>Client library for Azure Service Bus.</Description>
    <AssemblyTitle>Nimbus.MessageContracts</AssemblyTitle>
    <AssemblyName>Nimbus.MessageContracts</AssemblyName>
    <PackageId>Nimbus.MessageContracts</PackageId>
    <Authors>Damian Maclennan, Andrew Harcourt</Authors>
    <PackageProjectUrl>http://nimbusapi.com/</PackageProjectUrl>
    <PackageTags>azure,servicebus,autofac</PackageTags>
    <TargetFrameworks>netstandard2.0; net452</TargetFrameworks>
    <VersionPrefix>3.0.1</VersionPrefix>
    <VersionSuffix>issue-275.1</VersionSuffix>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Fody" Version="2.2.1" />
    <PackageReference Include="NullGuard.Fody" Version="1.6.3" />
    <PackageReference Include="Virtuosity.Fody" Version="1.20.0" />
  </ItemGroup>
</Project>